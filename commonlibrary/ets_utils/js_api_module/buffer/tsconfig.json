{
"compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "rootDir": "./src",
    "outDir": "./out",      /* Specify an output folder for all emitted files. */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitThis": false,
    "suppressImplicitAnyIndexErrors": true
 }
}

