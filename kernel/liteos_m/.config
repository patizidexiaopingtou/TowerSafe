
#
# Compiler
#
LOSCFG_COMPILER_GCC=y
LOSCFG_CROSS_COMPILE=""
# LOSCFG_COMPILER_CLANG_LLVM is not set
# LOSCFG_COMPILER_ICCARM is not set
# LOSCFG_COMPILE_DEBUG is not set
LOSCFG_COMPILE_OPTIMIZE=y
LOSCFG_COMPILE_OPTIMIZE_SIZE=y
LOSCFG_COMPILE_LTO=y
# end of Compiler

#
# Platform
#
LOSCFG_PLATFORM="virt"
LOSCFG_PRODUCT_NAME="xtensa_esp32"
LOSCFG_DEVICE_COMPANY="qemu"
# LOSCFG_PLATFORM_QEMU_ARM_VIRT_CM7 is not set
# LOSCFG_PLATFORM_QEMU_ARM_VIRT_CM4 is not set
# LOSCFG_PLATFORM_QEMU_ARM_VIRT_CM55 is not set
# LOSCFG_PLATFORM_QEMU_RISCV32_VIRT is not set
# LOSCFG_PLATFORM_QEMU_CSKY_SMARTL is not set
LOSCFG_PLATFORM_QEMU_XTENSA_ESP32=y
LOSCFG_PRODUCT_QEMU_XTENSA_ESP32=y
# LOSCFG_ARCH_FPU_VFP_V3 is not set
LOSCFG_ARCH_FPU_VFP_V4=y
LOSCFG_ARCH_XTENSA=y

#
# Extra Configurations
#
# LOSCFG_ARCH_FPU_DISABLE is not set
LOSCFG_SOC_COMPANY="esp"
LOSCFG_BOARD_TowerSafe=y
LOSCFG_ENABLE_LVGL=y
LOSCFG_USE_DIEMIT_APPLICATION=y
# LOSCFG_NIOBEU4_APPLICATION_004 is not set
# LOSCFG_NIOBEU4_APPLICATION_101 is not set
# LOSCFG_NIOBEU4_APPLICATION_102 is not set
# LOSCFG_NIOBEU4_APPLICATION_106 is not set
# LOSCFG_TowerSafe_APPLICATION_005 is not set
# LOSCFG_TowerSafe_APPLICATION_006 is not set
# LOSCFG_TowerSafe_APPLICATION_001 is not set
# LOSCFG_TowerSafe_APPLICATION_088 is not set
LOSCFG_TowerSafe_APPLICATION_089=y
# LOSCFG_TowerSafe_APPLICATION_009 is not set
LOSCFG_DIEMIT_APPLICATION_NAME="irrigation_system_app"
# LOSCFG_BOARD_M5EPD is not set
LOSCFG_SOC_SERIES="esp32_u4wdh"
LOSCFG_SOC_SERIES_ESP32=y
# LOSCFG_SOC_SERIES_RK22XX is not set
LOSCFG_SOC_COMPANY_ESPRESSIF=y
# LOSCFG_SOC_ESP32_U4WDH is not set
LOSCFG_SOC_ESP32_D0WDQ6=y
# LOSCFG_SOC_ESP32_PICO is not set
# LOSCFG_QUICK_START is not set
# end of Platform

#
# Kernel
#
LOSCFG_KERNEL_EXTKERNEL=y
# LOSCFG_KERNEL_BACKTRACE is not set
LOSCFG_BACKTRACE_TYPE=0
# LOSCFG_KERNEL_CPPSUPPORT is not set
# LOSCFG_KERNEL_SIGNAL is not set
# LOSCFG_KERNEL_CPUP is not set
LOSCFG_KERNEL_PM=y
# LOSCFG_KERNEL_PM_IDLE is not set
LOSCFG_KERNEL_PM_TASK_PTIORITY=1
LOSCFG_KERNEL_PM_TASK_STACKSIZE=1024
# LOSCFG_KERNEL_PM_DEBUG is not set
# LOSCFG_PLATFORM_EXC is not set
# LOSCFG_KERNEL_LMK is not set
# LOSCFG_KERNEL_TRACE is not set
# end of Kernel

#
# Lib
#
LOSCFG_LIB_LIBC=y
# end of Lib

#
# Compat
#
LOSCFG_KAL_CMSIS=y
# LOSCFG_LIBC_MUSL is not set
LOSCFG_LIBC_NEWLIB=y
LOSCFG_LIBC_NEWLIB_FS=y
# LOSCFG_LIBC_ICCARM is not set
LOSCFG_POSIX_API=y
# LOSCFG_POSIX_THREAD_API is not set
# LOSCFG_POSIX_SEM_API is not set
# LOSCFG_POSIX_CLOCK_API is not set
LOSCFG_POSIX_MQUEUE_API=y
LOSCFG_POSIX_PIPE_API=y
LOSCFG_POSIX_FS_API=y
# end of Compat

#
# FileSystem
#
LOSCFG_FS_VFS=y
LOSCFG_FS_LOCK_TIMEOUT=-1
# LOSCFG_FS_SUPPORT_MOUNT_TARGET_RECURSIVE is not set
LOSCFG_FS_LITTLEFS=y
LOSCFG_LFS_MAX_MOUNT_SIZE=1
LOSCFG_LFS_MAX_OPEN_FILES=32
# LOSCFG_FS_FAT is not set
LOSCFG_SUPPORT_LITTLEFS=y
# end of FileSystem

#
# Net
#
LOSCFG_NET_LWIP=y
LOSCFG_NET_LWIP_SACK=y
# end of Net

#
# Debug
#
# LOSCFG_GDB is not set
LOSCFG_PLATFORM_ADAPT=y
LOSCFG_ENABLE_MAGICKEY=y
# LOSCFG_SAVE_EXCINFO is not set
LOSCFG_DEBUG_VERSION=y
# LOSCFG_DEBUG_KERNEL is not set
LOSCFG_NET_LWIP_SACK_TFTP=y
# LOSCFG_DEBUG_HOOK is not set
LOSCFG_SCHED_DEBUG=y
LOSCFG_USER_INIT_DEBUG=y
LOSCFG_SHELL_CMD_DEBUG=y
# LOSCFG_DEBUG_TOOLS is not set
# LOSCFG_MEM_DEBUG is not set
LOSCFG_SHELL=y
LOSCFG_USE_SHELL=y

#
# Functionality of Shell
#
LOSCFG_SHELL_PRIO=9
LOSCFG_SHELL_LK=y
# LOSCFG_SHELL_DMESG is not set
# end of Functionality of Shell
# end of Debug

#
# Driver
#
LOSCFG_DRIVERS=y
LOSCFG_DRIVERS_HDF=y
LOSCFG_DRIVERS_HDF_CONFIG_MACRO=y
LOSCFG_DRIVERS_HDF_PLATFORM=y
LOSCFG_DRIVERS_HDF_PLATFORM_GPIO=y
LOSCFG_DRIVERS_HDF_PLATFORM_I2C=y
# LOSCFG_DRIVERS_HDF_PLATFORM_UART is not set
LOSCFG_DRIVERS_HDF_PLATFORM_ADC=y
# LOSCFG_DRIVERS_HDF_PLATFORM_MMC is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_EMMC is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_PWM is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_RTC is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_DMAC is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_MIPI_DSI is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_MIPI_CSI is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_HISI_SDK is not set
LOSCFG_DRIVERS_HDF_PLATFORM_SPI=y
# LOSCFG_DRIVERS_HDF_PLATFORM_I2S is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_HDMI is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_WATCHDOG is not set
# LOSCFG_DRIVERS_HDF_PLATFORM_REGULATOR is not set
# LOSCFG_DRIVERS_HDF_STORAGE is not set
# end of Driver

#
# Security
#
# LOSCFG_SECURE is not set
# LOSCFG_MPU_ENABLE is not set
# end of Security

#
# Test
#
# LOSCFG_KERNEL_TEST is not set
# end of Test

#
# Stack Smashing Protector (SSP) Compiler Feature
#
# LOSCFG_CC_NO_STACKPROTECTOR is not set
# LOSCFG_CC_STACKPROTECTOR is not set
LOSCFG_CC_STACKPROTECTOR_STRONG=y
# LOSCFG_CC_STACKPROTECTOR_ALL is not set
# end of Stack Smashing Protector (SSP) Compiler Feature
